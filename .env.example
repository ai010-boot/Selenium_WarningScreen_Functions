# 环境变量配置示例
# 复制此文件为 .env 并填入实际值

# 测试环境
ENV=test

# 浏览器配置
BROWSER=chrome
HEADLESS=false

# 测试账号（不要提交实际密码到版本控制）
TEST_USERNAME=testuser
TEST_PASSWORD=Test@123

# 数据库配置（如需要）
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=
DB_NAME=test_db

# API 配置（如需要）
API_BASE_URL=https://aiot.aiysyd.cn/screen/login
API_KEY=your_api_key_here

# 其他配置
SCREENSHOT_ON_FAILURE=true
LOG_LEVEL=INFO
